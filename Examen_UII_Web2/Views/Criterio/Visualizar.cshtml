@model Examen_UII_Web2.Models.Criterio
@{
    ViewBag.Title = "Visualizar";
}

<h2>Visualizar</h2>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="~/Criterio/Index">Criterio</a>
        </li>
        <li class="breadcrumb-item" active>@Model.nombre_criterio</li>

    </ol>

</nav>
<div class="card">
    <div class="card-header">
        Informacion detallada del usuario

    </div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.criterio_id</dd>
            <dt>Modelo</dt>
            <dd>@Model.Modelo.nombre_modelo</dd>
            <dt>Nombre</dt>
            <dd>@Model.nombre_criterio</dd>
        </dl>

    </div>

</div>
